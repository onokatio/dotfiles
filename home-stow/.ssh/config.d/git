Host github.com
	Hostname ssh.github.com
	User git
	Port 443
	VisualHostKey no

Host gitlab.com
	Hostname gitlab.com
	User git
	IdentityFile ~/.ssh/gitlab
	IdentitiesOnly yes

Host bitbucket.org
	Hostname bitbucket.org
	User git
	IdentityFile ~/.ssh/bitbucket
	IdentitiesOnly yes

Host aur.archlinux.org
  IdentityFile ~/.ssh/aur
  User onokatio

Host git.heroku.com
	User git
	IdentityFile ~/.ssh/github
