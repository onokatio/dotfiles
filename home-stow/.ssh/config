#Host office.pfidev.jp *.mn1b.pfn.io *.mn1s.pfn.io *.mnj.pfn.io *.mn1a.pfn.io bastion-aws.pfn.io
#User tmaruyama

Host kdev-micro
	User katio
	Port 443
	Hostname 35.212.253.240

Host 10.* 192.168.*.* 172.31.* 172.30.* 172.2?.* 172.1?.*
	StrictHostKeyChecking no
	UserKnownHostsFile=/dev/null
	LogLevel Error

Host *
	SetEnv TERM=xterm-256color
	#UseKeychain yes
	Compression yes
	TCPKeepAlive yes
#	LogLevel VERBOSE
	Compression yes
	ServerAliveInterval 15
	ServerAliveCountMax 3
	ConnectionAttempts 1
	ExitOnForwardFailure yes
	ForwardAgent yes
	GSSAPIAuthentication no
	ServerAliveInterval 10
	ControlMaster auto
	#ControlPath ~/.ssh/master_%r@%h:%p
	ControlPersist 5m
	AddressFamily inet
	AddKeysToAgent yes
	HashKnownHosts yes
	Ciphers +aes256-cbc
	#Macs 
	KexAlgorithms +diffie-hellman-group14-sha1,diffie-hellman-group1-sha1,diffie-hellman-group-exchange-sha1
	RekeyLimit default 600
	PubkeyAcceptedKeyTypes +ssh-rsa
	HostkeyAlgorithms +ssh-rsa
	PubkeyAcceptedAlgorithms +ssh-rsa
