
Host github.com
	Hostname ssh.github.com
	User git
	Port 443
	IdentityFile ~/.ssh/github
	TCPKeepAlive yes
	IdentitiesOnly yes
	ProxyCommand socat STDIO SOCKS4A:127.0.0.1:%h:%p,socksport=9050

Host gitlab.com
	Hostname gitlab.com
	User git
	Port 22
	IdentityFile ~/.ssh/gitlab
	TCPKeepAlive yes
	IdentitiesOnly yes

